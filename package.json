{
  "name": "digicomob",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postuninstall": "yarn run infinite-flatlist-patch",
    "postinstall": "npx jetify && yarn run infinite-flatlist-patch",
    "start": "REACT_DEBUGGER='rndebugger-open --open --port 8081' react-native start",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "android:emulator": "$ANDROID_HOME/emulator/emulator @Nexus_5X_API_28",
    "android:shake": "adb shell input keyevent 82",
    "android:adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:8081 tcp:8081",
    "test": "jest --verbose --coverage",
    "test:update": "jest --verbose --coverage --updateSnapshot",
    "test:watch": "jest --verbose --watch",
    "coverage": "jest --verbose --coverage && open ./coverage/lcov-report/index.html",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "lint": "npx tslint --project tsconfig.json",
    "android:beta": "bundle exec fastlane android beta",
    "ios:beta": "bundle exec fastlane ios beta",
    "fastlane:beta": "yarn android:beta && yarn ios:beta"
  },
  "dependencies": {
    "@apollo/react-components": "^3.1.3",
    "@apollo/react-hoc": "^3.1.3",
    "@apollo/react-hooks": "^3.1.3",
    "@react-native-community/async-storage": "^1.6.1",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-link-state": "^0.4.2",
    "date-fns": "^2.5.1",
    "formik": "^2.0.3",
    "graphql": "^14.5.7",
    "graphql-tag": "^2.10.1",
    "lodash": "^4.17.15",
    "lottie-ios": "3.1.5",
    "lottie-react-native": "^3.2.1",
    "react": "16.11.0",
    "react-native": "0.61.4",
    "react-native-animatable": "^1.3.3",
    "react-native-animated-loader": "^0.0.8",
    "react-native-collapsible": "^1.5.1",
    "react-native-fast-image": "^7.0.2",
    "react-native-gesture-handler": "^1.5.0",
    "react-native-iphone-x-helper": "^1.2.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-linear-gradient": "^2.5.3",
    "react-native-loader": "^1.2.1",
    "react-native-masked-text": "^1.13.0",
    "react-native-material-textfield": "^0.16.1",
    "react-native-modal-datetime-picker": "^7.5.2",
    "react-native-reanimated": "^1.4.0",
    "react-native-slider": "^0.11.0",
    "react-native-sortable-listview": "^0.2.8",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^9.13.3",
    "react-native-swift": "^1.2.2",
    "react-native-swiper": "^1.5.14",
    "react-native-tab-view": "^2.10.0",
    "react-native-track-player": "^1.1.4",
    "react-native-vector-icons": "https://github.com/RageOfJustice/react-native-vector-icons",
    "react-native-webview": "^7.4.2",
    "react-navigation": "^4.0.9",
    "react-navigation-animated-switch": "^0.3.2",
    "react-navigation-header-buttons": "^3.0.4",
    "react-navigation-stack": "^1.10.3",
    "rn-sliding-up-panel": "^2.3.3",
    "styled-components": "^4.4.1",
    "url": "^0.11.0",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.0",
    "@babel/runtime": "^7.7.1",
    "@storybook/react-native": "^5.2.4",
    "@testing-library/react-native": "^5.0.0",
    "@types/jest": "^24.0.22",
    "@types/lodash": "^4.14.144",
    "@types/react": "^16.9.11",
    "@types/react-native": "^0.60.22",
    "@types/react-native-material-textfield": "^0.15.0",
    "@types/react-native-vector-icons": "^6.4.4",
    "@types/react-test-renderer": "^16.9.1",
    "@types/styled-components": "^4.1.21",
    "@types/yup": "^0.26.24",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "husky": "^3.0.9",
    "jest": "^24.8.0",
    "jest-styled-components": "^6.3.3",
    "jetifier": "^1.6.4",
    "lint-staged": "^9.4.2",
    "metro-react-native-babel-preset": "^0.57.0",
    "prettier": "^1.16.4",
    "react-native-infinite-flatlist-patch": "^1.0.3",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "16.11.0",
    "reactotron-react-native": "^4.0.1",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.1.0",
    "tslint-react-hooks": "^2.2.1",
    "typescript": "^3.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "tslint --fix --project tsconfig.json",
      "git add"
    ]
  }
}
